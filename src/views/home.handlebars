<!--IMPRIMIR LA LISTA DE PRODUCTOS HASTA EL MOMENTO-->

<div class="products-container">
  <h1>Productos en stock</h1>

  {{#each products}}
    <div class="card">
      <div>Id: {{this.id}}</div>
      <div>Nombre: {{this.title}}</div>
      <div>Descripcion: {{this.description}}</div>
      <div>Precio: ${{this.price}}</div>
      <div>Imagen: {{this.thumbnail}}</div>
      <div>Codigo: {{this.code}}</div>
      <div>Stock: {{this.stock}}</div>
    </div>
  {{/each}}

</div>

<div>
  <h1>Bienvenido!</h1>
  <input type="text" id="textBox" />
  <p id="log"></p>

</div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>