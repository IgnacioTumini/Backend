<h1>Cart</h1>

<table class="table">
  <thead>
    <tr>

      <th scope="col">Titulo</th>
      <th scope="col">Precio</th>
      <th scope="col">Cantidad</th>
    </tr>
  </thead>
  <tbody>

    {{#each plainCart}}
      <tr>

        <td>{{this.product.title}}</td>
        <td>{{this.product.price}}</td>
        <td>{{this.quantity}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>